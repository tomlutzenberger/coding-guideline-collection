;
; Editorconfig for Typo3 PHP coding standard
; Follows PSR-2
;
; For details see:
; https://docs.typo3.org/typo3cms/CodingGuidelinesReference/PhpFileFormatting/GeneralRequirementsForPhpFiles/Index.html
;


[*.php]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8


; Supported By A Limited Number of Editors
max_line_length = 130
