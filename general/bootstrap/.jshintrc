/*
 * JSHint Rules for Bootstrap coding standard
 *
 * For details see:
 * https://github.com/twbs/bootstrap/blob/master/js/.jshintrc
 */

{
  "asi"      : true,
  "browser"  : true,
  "eqeqeq"   : false,
  "eqnull"   : true,
  "es3"      : true,
  "expr"     : true,
  "jquery"   : true,
  "latedef"  : true,
  "laxbreak" : true,
  "nonbsp"   : true,
  "strict"   : true,
  "undef"    : true,
  "unused"   : true
}
